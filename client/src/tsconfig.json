{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "isolatedModules": true,
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
